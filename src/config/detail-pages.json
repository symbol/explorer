{
    "pages": [
        {
            "path": "/account/:address",
            "name": "account-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Account Detail",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "address",
                            "addressHeight",
                            "publicKey",
                            "importance",
                            "type",
                            "linkedAccountKey"
                        ],
                        "managerGetter": "account/info",
                        "pagination": false,
                        "errorMessage": "Account is unknown to the network"
                    },
                    {
                        "type": "Card",
                        "title": "Multisig Cosignatories",
                        "fullWidth": false,
                        "body": "TableInfoView",
                        "fields": [
                            "minApproval",
                            "minRemoval",
                            "cosignatories",
                            "multisigAccounts"
                        ],
                        "managerGetter": "account/multisig",
                        "pagination": false,
                        "errorMessage": "Address is not belong to Multisig"
                    },
                    {
                        "type": "Card",
                        "title": "Owned Mosaics",
                        "fullWidth": false,
                        "body": "TableListView",
                        "managerGetter": "account/OwnedMosaic",
                        "fields": [
                            "mosaicId",
                            "amount",
                            "mosaicAliasName"
                        ],
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Owned Namespaces",
                        "fullWidth": false,
                        "body": "TableListView",
                        "managerGetter": "account/OwnedNamespace",
                        "fields": [
                            "namespaceName",
                            "status",
                            "duration",
                            "registrationType"
                        ],
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Account Partial",
                        "fullWidth": true,
                        "body": "TableListView",
                        "managerGetter": "account/partialTransactions",
                        "fields": [
                            "signer",
                            "transactionType",
                            "transactionHash",
                            "deadline",
                            "transactionBody"
                        ],
                        "timelinePagination": true
                    },
                    {
                        "type": "Card",
                        "title": "Account Restriction",
                        "fullWidth": true,
                        "body": "TableListView",
                        "managerGetter": "account/restrictions",
                        "pagination": false,
                        "pageSize": 5,
                        "errorMessage": "Account Restrictions is no available."
                    },
                    {
                        "type": "Card",
                        "title": "Metadata Entries",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "metadataId",
                            "scopedMetadataKey",
                            "senderAddress",
                            "targetAddress",
                            "metadataValue"
                        ],
                        "managerGetter": "account/metadatas",
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Importance History",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "recalculationBlock",
                            "totalFeesPaid",
                            "beneficiaryCount",
                            "importanceScore"
                        ],
                        "managerGetter": "account/info",
                        "dataGetter": "account/getActivityBucketList",
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Transactions",
                        "fullWidth": true,
                        "header": [
                            {
                                "type": "filter-tmp-disabled",
                                "filterOptionsGetter": "account/transactionFilterOptions",
                                "filterValueGetter": "account/transactionFilterValue",
                                "filterChangeAction": "account/setTransactionFilterValue"
                            }
                        ],
                        "body": "TableListView",
                        "fields": [
                            "deadline",
                            "transactionHash",
                            "transactionType",
                            "direction"
                        ],
                        "managerGetter": "account/transactions",
                        "timelinePagination": true
                    }
                ]
            }
        },
        {
            "path": "/block/:height",
            "name": "block-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Block Detail",
                        "fullWidth": true,
                        "header": [
                            {
                                "type": "Pagination",
                                "canFetchPrevious": true,
                                "canFetchNext": true,
                                "nextPageAction": "block/nextBlock",
                                "previousPageAction": "block/previousBlock"
                            }
                        ],
                        "body": "TableInfoView",
                        "fields": [
                            "height",
                            "date",
                            "totalFee",
                            "difficulty",
                            "feeMultiplier",
                            "transactions",
                            "harvester",
                            "blockHash"
                        ],
                        "pagination": false,
                        "managerGetter": "block/info",
                        "errorMessage": "Block does not exist"
                    },
                    {
                        "type": "Card",
                        "title": "Block Transactions",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "deadline",
                            "transactionId",
                            "transactionHash",
                            "type"
                        ],
                        "managerGetter": "block/blockTransactions",
                        "timelinePagination": true
                    },
                    {
                        "type": "Card",
                        "title": "Balance Change Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/balanceChangeReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": false,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Balance Transfer Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/balanceTransferReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Inflation Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/inflationReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Artifact Expiry Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/artifactExpiryReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Resolution Statement",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/resolutionStatement",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    }
                ]
            }
        },
        {
            "path": "/mosaic/:mosaicId",
            "name": "mosaic-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Mosaic Detail",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "mosaicAliasName",
                            "divisibility",
                            "address",
                            "supply",
                            "relativeAmount",
                            "revision",
                            "startHeight",
                            "duration",
                            "supplyMutable",
                            "transferable",
                            "restrictable"
                        ],
                        "managerGetter": "mosaic/info",
                        "pagination": false,
                        "errorMessage": "Mosaic does not exist"
                    },
                    {
                        "type": "Card",
                        "title": "Mosaic Restriction",
                        "fullWidth": false,
                        "body": "TableInfoView",
                        "managerGetter": "mosaic/restrictions",
                        "pagination": false,
                        "errorMessage": "Mosaic does not exist"
                    },
                    {
                        "type": "Card",
                        "title": "Mosaic Restriction List",
                        "fullWidth": false,
                        "body": "TableListView",
                        "managerGetter": "mosaic/restrictions",
                        "dataGetter": "mosaic/getMosaicRestrictionList",
                        "pagination": false,
                        "errorMessage": "Mosaic Global Restrictions is no available."
                    },
                    {
                        "type": "Card",
                        "title": "Metadata Entries",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "metadataId",
                            "scopedMetadataKey",
                            "targetId",
                            "senderAddress",
                            "targetAddress",
                            "metadataValue"
                        ],
                        "managerGetter": "mosaic/metadatas",
                        "pagination": true,
                        "pageSize": 5
                    }
                ]
            }
        },
        {
            "path": "/namespace/:namespaceId",
            "name": "namespace-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Namespace Detail",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "owneraddress",
                            "namespaceName",
                            "namespaceId",
                            "registrationType",
                            "status",
                            "aliasType",
                            "aliasAddress",
                            "aliasMosaic",
                            "startHeight",
                            "expiredInBlock",
                            "beforeEndHeight",
                            "endHeight"
                        ],
                        "managerGetter": "namespace/info",
                        "pagination": false,
                        "errorMessage": "Namespace does not exist"
                    },
                    {
                        "type": "Card",
                        "title": "Namespace Level",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "name",
                            "namespaceId",
                            "parentId"
                        ],
                        "managerGetter": "namespace/namespaceLevel",
                        "pagination": false
                    },
                    {
                        "type": "Card",
                        "title": "Metadata Entries",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "metadataId",
                            "scopedMetadataKey",
                            "targetId",
                            "senderAddress",
                            "targetAddress",
                            "metadataValue"
                        ],
                        "managerGetter": "namespace/metadatas",
                        "pagination": true,
                        "pageSize": 5
                    }
                ]
            }
        },
        {
            "path": "/transaction/:transactionHash",
            "name": "transaction-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Transaction Info",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "blockHeight",
                            "transactionHash",
                            "transactionId",
                            "date",
                            "deadline",
                            "effectiveFee",
                            "signature",
                            "signer",
                            "status",
                            "confirm"
                        ],
                        "managerGetter": "transaction/info",
                        "pagination": false,
                        "errorMessage": "Transaction does not exist"
                    },
                    {
                        "type": "Card",
                        "title": "Transaction Detail",
                        "fullWidth": false,
                        "body": "TableInfoView",
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/transactionDetail",
                        "pagination": false
                    },
                    {
                        "type": "Card",
                        "title": "Mosaics",
                        "fullWidth": false,
                        "body": "TableListView",
                        "fields": [
                            "mosaicId",
                            "amount",
                            "mosaicAliasName"
                        ],
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/transferMosaics",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Aggregate Inner Transactions",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "transactionId",
                            "type",
                            "signer",
                            "transactionBody"
                        ],
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/aggregateInnerTransactions",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Aggregate Cosignatures",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "signature",
                            "signer"
                        ],
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/aggregateCosignatures",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    }
                ]
            }
        }
    ]
}